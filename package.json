{
	"name": "@krishna_ghl/sample-ui",
	"title": "Sample UI",
	"description": "Extending Naive ui",
	"keywords": [
		"naive-ui",
		"component library",
		"ui framework",
		"ui",
		"vue",
		"vue3"
	],
	"version": "0.18.2",
	"license": "MIT",
	"type": "module",
	"main": "lib/index.cjs",
	"module": "es/index.mjs",
	"types": "es/index.d.ts",
	"files": [
		"dist/*",
		"lib/*",
		"es/*",
		"types/*",
		"!types/shims-*.d.ts"
	],
	"exports": {
		".": {
			"require": {
				"default": "./lib/index.cjs",
				"types": "./lib/index.d.ts"
			},
			"import": {
				"default": "./es/index.mjs",
				"types": "./es/index.d.ts"
			}
		},
		"./unplugin": {
			"require": {
				"default": "./lib/unplugin.cjs",
				"types": "./lib/unplugin.d.ts"
			},
			"import": {
				"default": "./es/unplugin.mjs",
				"types": "./es/unplugin.d.ts"
			}
		},
		"./types/*": {
			"default": "./types/*"
		},
		"./*": {
			"require": "./lib/*",
			"import": "./es/*"
		}
	},
	"engines": {
		"node": ">=20"
	},
	"scripts": {
		"build": "pnpm run gen-dist && pnpm run gen-dts && pnpm run gen-volar",
		"gen-dist": "rollup -c",
		"gen-dts": "vue-tsc -p tsconfig.dts-cjs.json && vue-tsc -p tsconfig.dts-esm.json",
		"gen-volar": "esbuild scripts/gen-volar.js --bundle --platform=node --tsconfig=tsconfig.esbuild.json | node",
		"demo": "vite --config demo/vite.config.ts",
		"prepublishOnly": "pnpm run build"
	},
	"devDependencies": {
		"@babel/core": "^7.23.9",
		"@babel/preset-env": "^7.23.9",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-commonjs": "^25.0.7",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-replace": "^5.0.5",
		"@rollup/plugin-terser": "^0.4.4",
		"@rollup/plugin-typescript": "^11.1.6",
		"@rushstack/eslint-patch": "^1.7.2",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "~18.16.0",
		"@vitejs/plugin-vue": "^5.0.4",
		"@vitejs/plugin-vue-jsx": "^3.1.0",
		"@vue/babel-plugin-jsx": "^1.2.1",
		"@vue/compiler-sfc": "^3.4.20",
		"@vue/eslint-config-prettier": "^9.0.0",
		"@vue/eslint-config-standard": "^8.0.1",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/runtime-core": "^3.4.20",
		"deepmerge": "^4.3.1",
		"esbuild": "^0.20.1",
		"eslint": "^8.57.0",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-vue": "^9.22.0",
		"highlight.js": "^11.9.0",
		"naive-ui": "^2.40.1",
		"prettier": "^3.2.5",
		"rollup": "^3.29.4",
		"rollup-plugin-esbuild": "^6.1.1",
		"rollup-plugin-string": "^3.0.0",
		"rollup-plugin-vue": "^6.0.0",
		"treemate": "^0.3.11",
		"typescript": "~5.3.0",
		"vite": "^5.1.4",
		"vite-svg-loader": "^5.1.0",
		"vue": "^3.4.20",
		"vue-router": "^4.3.0",
		"vue-tsc": "^1.8.27"
	},
	"peerDependencies": {
		"naive-ui": "^2.37.0",
		"vue": "^3.2.0"
	},
	"sideEffects": false,
	"unpkg": "dist/index.prod.js",
	"jsdelivr": "dist/index.prod.js",
	"directories": {
		"doc": "docs",
		"lib": "lib"
	},
	"dependencies": {
		"eslint-config-prettier": "^9.1.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-import-resolver-custom-alias": "^1.3.2",
		"eslint-import-resolver-node": "^0.3.9",
		"eslint-module-utils": "^2.8.0",
		"eslint-plugin-es": "^4.1.0",
		"eslint-plugin-import": "^2.28.0",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-scope": "^7.2.2",
		"eslint-utils": "^3.0.0",
		"eslint-visitor-keys": "^3.4.3",
		"prettier-linter-helpers": "^1.0.0",
		"vue-eslint-parser": "^9.3.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/SarathSantoshDamaraju/sample-ui.git"
	},
	"author": "",
	"bugs": {
		"url": "https://github.com/SarathSantoshDamaraju/sample-ui/issues"
	},
	"homepage": "https://github.com/SarathSantoshDamaraju/sample-ui#readme",
	"volta": {
		"node": "20.18.0"
	}
}